@model Proyecto_VitalPets_PIV.Models.Veterinario

@{
    ViewData["Title"] = "Detalles del Veterinario";
}

<h1>Detalles de Veterinario</h1>

<div>
    <h4>Veterinario</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Nombre</dt>
        <dd class="col-sm-10">@Model.Nombre</dd>

        <dt class="col-sm-2">Especialidad</dt>
        <dd class="col-sm-10">@Model.Especialidad</dd>
    </dl>
</div>

<div>
    @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
    <a asp-action="Index">Volver a la Lista</a>
</div>
